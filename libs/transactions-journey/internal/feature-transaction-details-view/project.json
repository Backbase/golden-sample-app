  "prefix": "backbase",
  "projectType": "library",
  "tags": ["scope:transaction-details-view-journey", "type:feature"],
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist/internal/{projectRoot}"],
