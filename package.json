{
  "name": "golden-sample-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "analyze": "webpack-bundle-analyzer dist/golden-sample-app/stats.json",
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "ng serve",
    "start:docker": "docker run -p 8080:8080 golden-sample-app",
    "test": "nx run-many --target=test --all",
    "build": "nx build --configuration production",
    "build:stats": "ng build --configuration production --stats-json",
    "build:docker": "docker build -t golden-sample-app:latest .",
    "xi18n": "ng extract-i18n golden-sample-app --output-path=apps/golden-sample-app/src/locale && xliffmerge --profile apps/golden-sample-app/src/xliffmerge.json nl-NL",
    "e2e": "npx playwright test",
    "lint": "nx run-many --all --target=lint",
    "format": "prettier --write \"{apps,libs}/**/*.{ts,md,html}\" \"!{apps,libs}/**/coverage\" ",
    "format:check": "prettier --list-different \"{apps,libs}/**/*.{ts,md}\" \"!{apps,libs}/**/coverage\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.3.11",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "^13.3.11",
    "@angular/compiler": "^13.3.11",
    "@angular/core": "^13.3.11",
    "@angular/forms": "^13.3.11",
    "@angular/localize": "^13.3.11",
    "@angular/platform-browser": "^13.3.11",
    "@angular/platform-browser-dynamic": "^13.3.11",
    "@angular/router": "^13.3.11",
    "@backbase/accessgroup-common-ang": "^6.11.3",
    "@backbase/accessgroup-ui": "^5.11.4",
    "@backbase/arrangement-manager-http-ang": "^2.3.0",
    "@backbase/data-ang": "^4.3.0",
    "@backbase/foundation-ang": "^7.0.0-beta.1",
    "@backbase/positive-pay-check-v1-client-ang": "^5.1.0",
    "@backbase/positive-pay-journey-ang": "^2.0.1",
    "@backbase/select-context-widget-ang": "^5.6.2",
    "@backbase/ui-ang": "^8.0.0-pr.68",
    "@material/animation": "^3.0.0",
    "@ng-bootstrap/ng-bootstrap": "~11.0.1",
    "@ng-select/ng-select": "^8.1.1",
    "@ngrx/component-store": "^13.2.0",
    "@ngrx/effects": "^13.2.0",
    "@ngrx/store": "^13.2.0",
    "angular-oauth2-oidc": "^13.0.1",
    "bignumber.js": "^9.0.1",
    "bootstrap": "^4.6.2",
    "ngx-cookie-service": "^12.0.3",
    "ngx-mask": "^13.0.0",
    "quill": "^1.3.7",
    "rxjs": "~7.5.6",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.9",
    "@angular-eslint/builder": "13.5.0",
    "@angular-eslint/eslint-plugin": "13.5.0",
    "@angular-eslint/eslint-plugin-template": "13.5.0",
    "@angular-eslint/schematics": "13.5.0",
    "@angular-eslint/template-parser": "13.5.0",
    "@angular/cli": "^13.3.9",
    "@angular/compiler-cli": "^13.3.11",
    "@angular/language-service": "^13.3.11",
    "@nrwl/angular": "13.10.3",
    "@nrwl/cli": "13.10.3",
    "@nrwl/eslint-plugin-nx": "13.10.3",
    "@nrwl/jest": "13.10.3",
    "@nrwl/linter": "13.10.3",
    "@nrwl/tao": "12.10.1",
    "@nrwl/workspace": "13.10.3",
    "@playwright/test": "^1.18.1",
    "@types/jest": "^27.4.0",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-playwright": "^0.7.1",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jest": "~27.4.7",
    "jest-preset-angular": "11.1.1",
    "ng-packagr": "^13.3.1",
    "ngx-i18nsupport": "^0.17.1",
    "prettier": "^2.3.1",
    "ts-jest": "^27.1.3",
    "typescript": "~4.6.4",
    "tslib": "^2.4.0",
    "webpack-bundle-analyzer": "^4.5.0"
  }
