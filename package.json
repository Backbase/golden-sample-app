{
  "name": "golden-sample-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "analyze": "webpack-bundle-analyzer dist/golden-sample-app/stats.json",
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "start": "ng serve",
    "start:mocks": "nx serve -c mocks",
    "start:nl": "ng serve golden-sample-app:serve:nl",
    "start:docker": "docker-compose up --build",
    "test": "nx run-many --target=test --all",
    "build": "nx build --configuration production",
    "build:stats": "ng build --configuration production --stats-json",
    "build:docker": "docker build -t golden-sample-app:latest .",
    "mock-server": "node ./mock-server/mock-server.js -- golden-sample-app",
    "affected:build": "nx affected:build --configuration=production",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "xi18n": "ng extract-i18n golden-sample-app --output-path=apps/golden-sample-app/src/locale && xliffmerge --profile apps/golden-sample-app/src/xliffmerge.json nl-NL",
    "e2e": "nx e2e",
    "lint": "nx run-many --all --target=lint",
    "format": "prettier --write \"{apps,libs}/**/*.{ts,md,html}\" \"!{apps,libs}/**/coverage\" ",
    "format:check": "prettier --list-different \"{apps,libs}/**/*.{ts,md}\" \"!{apps,libs}/**/coverage\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.12",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.2.12",
    "@angular/compiler": "^14.2.12",
    "@angular/core": "^14.2.12",
    "@angular/forms": "^14.2.12",
    "@angular/localize": "^14.2.12",
    "@angular/platform-browser": "^14.2.12",
    "@angular/platform-browser-dynamic": "^14.2.12",
    "@angular/router": "^14.2.12",
    "@backbase/accesscontrol-http-ang": "^3.3.2",
    "@backbase/accesscontrol-v3-http-ang": "^1.1.0",
    "@backbase/arrangement-manager-http-ang": "^3.3.1",
    "@backbase/foundation-ang": "^8.0.0",
    "@backbase/identity-auth": "^2.1.0",
    "@backbase/initiate-payment-journey-ang": "^3.0.5",
    "@backbase/select-context-widget-ang": "^7.0.4",
    "@backbase/transactions-http-ang": "^3.1.2",
    "@backbase/ui-ang": "^9.0.0-beta.1",
    "@material/animation": "^3.1.0",
    "@ng-bootstrap/ng-bootstrap": "^13.1.1",
    "@ng-select/ng-select": "^9.1.0",
    "@ngrx/component-store": "^14.0.2",
    "@ngrx/effects": "^14.0.2",
    "@ngrx/store": "^14.0.2",
    "@ngx-formly/core": "^6.0.4",
    "@popperjs/core": "^2.10.2",
    "@types/quill": "^1.3.10",
    "angular-oauth2-oidc": "^14.0.1",
    "bignumber.js": "^9.1.1",
    "bootstrap": "~5.2.0",
    "ngx-cookie-service": "^14.0.1",
    "ngx-mask": "^13.1.15",
    "ngx-quill": "^16.2.1",
    "quill": "^1.3.7",
    "rxjs": "^7.8.0",
    "ts-morph": "^15.0.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.8"
