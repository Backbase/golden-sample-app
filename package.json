    "@angular/platform-browser-dynamic": "18.2.9",
    "@angular/router": "18.2.9",
    "@backbase/accesscontrol-v3-http-ang": "^3.1.0",
    "@backbase/arrangement-manager-http-ang": "^5.0.0",
    "@backbase/foundation-ang": "^11.3.1",
    "@backbase/identity-auth": "^5.1.1",
    "@backbase/initiate-payment-journey-ang": "6.1.1",
    "@backbase/select-context": "10.2.0",
    "@backbase/transactions-http-ang": "^5.0.0",
    "@backbase/ui-ang": "^12.7.0",
    "@material/animation": "^3.0.0",
    "@ng-bootstrap/ng-bootstrap": "^17.0.0",
    "@ng-select/ng-select": "^13.0.0",
    "@ngrx/component-store": "18.1.1",
