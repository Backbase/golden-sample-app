    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.1",
    "jest-marbles": "^3.0.3",
    "jest-preset-angular": "^12.2.3",
    "ng-packagr": "^14.2.2",
    "ngx-i18nsupport": "^0.17.1",
    "nx": "14.8.8",
    "prettier": "^2.8.1",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4",
    "webpack-bundle-analyzer": "^4.5.0"
  }
