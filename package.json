    "@schematics/angular": "~19.2.0",
    "@types/jest": "29.5.14",
    "@types/node": "^22.17.2",
    "@typescript-eslint/utils": "^8.19.0",
    "allure-js-commons": "3.1.0",
    "allure-playwright": "3.1.0",
    "angular-eslint": "^19.8.1",
    "autoprefixer": "^10.4.0",
    "axe-html-reporter": "^2.2.11",
    "dotenv": "^16.4.7",
    "eslint": "^9.8.0",
