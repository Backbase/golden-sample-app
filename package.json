    "jest-marbles": "^3.0.3",
    "jest-preset-angular": "14.4.2",
    "markdown-include": "^0.4.3",
    "ng-packagr": "17.3.0",
    "ngx-i18nsupport": "^0.17.1",
    "nx": "18.3.5",
    "prettier": "^2.8.1",
    "ts-jest": "29.2.5",
