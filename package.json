{
  "name": "golden-sample-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:proxy:drupal": "ng serve -- --proxy-config ./proxy-local-drupal-adapter.conf.json",
    "start:wordpress": "ng serve -- --proxy-config ./proxy-local-wordpress.conf.json",
    "build": "ng build",
    "build:stats": "ng build --configuration production --stats-json",
    "analyze": "webpack-bundle-analyzer dist/golden-sample-app/stats.json",
    "build:docker": "docker build -t golden-sample-app:latest .",
    "start:docker": "docker run -p 8080:8080 golden-sample-app",
    "xi18n": "ng extract-i18n golden-sample-app --output-path=src/locale && xliffmerge --profile src/xliffmerge.json nl-NL",
    "test": "ng test --code-coverage=true",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:journeys": "ng build transactions-journey && ng build transfer-journey",
    "test:transfer:watch": "ng test transfer-journey --watch",
    "test:transactions:watch": "ng test transactions-journey --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.8",
    "@angular/cdk": "^12.2.8",
    "@angular/common": "~12.2.8",
    "@angular/compiler": "~12.2.8",
    "@angular/core": "~12.2.8",
    "@angular/forms": "~12.2.8",
    "@angular/localize": "~12.2.8",
    "@angular/platform-browser": "~12.2.8",
    "@angular/platform-browser-dynamic": "~12.2.8",
    "@angular/router": "~12.2.8",
    "@backbase/backbase-theme": "^2.282.2",
    "@backbase/data-ang": "^3.10.1",
    "@backbase/drupal-http-module-ang": "file:drupal-http-module-ang",
    "@backbase/foundation-ang": "^6.13.4",
    "@backbase/positive-pay-journey-ang": "^0.29.0",
    "@backbase/ui-ang": "^6.21.0",
    "@backbase/universal-ang": "^7.0.0",
    "@backbase/wordpress-http-module-ang": "file:wordpress-http-module-ang",
    "@ng-bootstrap/ng-bootstrap": "^9.0.2",
    "@ng-select/ng-select": "^6.1.0",
    "@ngrx/effects": "^12.4.0",
    "@ngrx/store": "^12.4.0",
    "angular-oauth2-oidc": "^12.1.0",
    "bignumber.js": "^9.0.1",
    "rxjs": "~6.6.6",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.6",
    "@angular-eslint/builder": "12.3.1",
    "@angular-eslint/eslint-plugin": "12.3.1",
    "@angular-eslint/eslint-plugin-template": "12.3.1",
    "@angular-eslint/schematics": "12.5.0",
    "@angular-eslint/template-parser": "12.3.1",
    "@angular/cli": "~12.2.6",
    "@angular/compiler-cli": "~12.2.8",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.20.20",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "eslint": "^7.26.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "https-proxy-agent": "^5.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.2.1",
    "ngx-i18nsupport": "^0.17.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.3.5",
    "webpack-bundle-analyzer": "^4.4.2"
  }
}
